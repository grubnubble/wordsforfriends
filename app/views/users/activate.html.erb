<h1>Verify log-in</h1>

<%= form_tag do %>

  <p>
    <%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email] %>
  </p>
  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </p>
  <p><%= submit_tag "Activate" %></p>

  <%= hidden_field :e_k, '', :value => params[:e_k] %>

<% end %>
